[Unit]
Description=Receive packets from IMPISH and write them to the database
After=network.target
StartLimitIntervalSec=0

[Service]
Type=simple
ExecStart=health_listener
EnvironmentFile=/usr/local/bin/grafana.env
StandardOutput=journal
StandardError=journal
Restart=on-failure
RestartSec=5
User=impish

[Install]
WantedBy=multi-user.target
